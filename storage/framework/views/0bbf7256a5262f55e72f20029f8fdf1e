    <?php echo $__env->make('alerts.success', array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>

    <?php $__env->startSection('content'); ?>


<div class="container-fluid">
    <div class="row"></div>
    <div class="row">
        <div class="col-md-30"></div>
        <div class="col-md-60">
            <div id="content"></div>
        </div>
        <div class="col-md-30"></div>
    </div>
    <div class="loading"></div>
